{
  "globals": {
    "ts-jest": {
      "tsconfig": "tsconfig.json",
      "babelConfig": true,
      "diagnostics": true
    }
  },
  "transform": {
    "^.+\\.tsx?$": "ts-jest",
    "^.+\\.jsx?$": "babel-jest"
  },
  "testRegex": "/__tests__/.*[^d].ts$",
  "moduleFileExtensions": ["ts", "js", "json", "node"]
}

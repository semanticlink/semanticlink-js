{
  "name": "semantic-link",
  "version": "0.0.6",
  "description": "A utility library for manipulating a list of links that form a semantic interface to a resource.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf ./lib",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semanticlink/semanticlink-js.git"
  },
  "keywords": [
    "hypermedia",
    "microformat",
    "REST",
    "atom",
    "API"
  ],
  "author": "toddb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/semanticlink/semanticlink-js/issues"
  },
  "homepage": "https://github.com/semanticlink/semanticlink-js#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^23.0.1",
    "prettier": "^1.13.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
